<% if (isLoggedIn) { %>
    <%- include("templates/header") %>
<% } else { %>
    <%- include("templates/header-beforeLogin") %>
<% } %>

<div class="bookmarks-title">My Bookmarks</div>

<% if (bookmarkedCourses.length > 0) { %>
    <% for(let i=0; i < bookmarkedCourses.length; i++) { %>
        <%- include('templates/bookmarks-list', {result: bookmarkedCourses[i], userBookmarks: userBookmarks })  %>
    <% } %>
<% } else { %>
        <p>You have no bookmarks yet.</p>
<% } %>
     
<%- include("templates/footer") %>
